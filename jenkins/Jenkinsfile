pipeline {

    agent none

    stages {

        stage('Build') {

            agent {

                docker {

                    image 'python:2-alpine'

                }

            }

            steps {

                sh 'python -m py_compile tryitout.py'

            }

        }

        stage('Test') {

            agent {

                docker {

                    image 'qnib/pytest'

                }

            }

            steps {

                
               sh "echo 'done steps' "

            }


        }

    }

}
